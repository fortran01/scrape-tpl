{
  "name": "lambda-frontend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/handler.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --external:pg-native && cp -r src/views dist/",
    "dev": "npm run build && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "aws",
    "lambda",
    "toronto-public-library",
    "tpl",
    "events"
  ],
  "author": "",
  "license": "ISC",
  "description": "AWS Lambda frontend for Toronto Public Library events calendar",
  "dependencies": {
    "ejs": "^3.1.10",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/pg": "^8.15.4",
    "esbuild": "^0.25.5",
    "jest": "^30.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
